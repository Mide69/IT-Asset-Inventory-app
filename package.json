{
  "name": "student-crud-api",
  "version": "1.0.0",
  "description": "Student CRUD REST API with Node.js and React frontend",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
